{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "quotemark": [
            true,
            "single"
        ],
        "new-parens": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "member-ordering": false,
        "no-console": [false, "log", "error"],
        "space-before-function-paren": false,
        "typedef-whitespace": false
    },
    "rulesDirectory": []
}